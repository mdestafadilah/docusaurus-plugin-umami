{
  "version": "1.0.0",
  "repository": "git://github.com/mdestafadilah/docusaurus-plugin-umami.git",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@mdestafadilah/docusaurus-plugin-umami",
  "author": "mdestafadilah",
  "module": "dist/docusaurus-plugin-umami.esm.js",
  "size-limit": [
    {
      "path": "dist/docusaurus-plugin-umami.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/docusaurus-plugin-umami.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@docusaurus/types": "2.0.0-beta.9",
    "@size-limit/preset-small-lib": "^7.0.3",
    "husky": "^7.0.4",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
